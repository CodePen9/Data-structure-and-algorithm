package com.jiang;

/*
 * title: 求第n个斐波那契数
 * 0 1 1 2 3 5 ....
 */
public class Main {
	
	//第一种算法
	public static int fib1(int n) {
		if(n <= 1) {
			return n;	
		}
		return fib1(n - 1) + fib1(n - 2);
	}
	
	//第二种算法
	public static int fib2(int n) { //3
		int first = 0;
		int second = 1;
		for(int i = 0;i < n - 1; i++) { //0<2 0   1
			int sum = first + second; // 1
			first = second; //1
			second = sum; // 1
		}
		return second;
	}
	
	public static void main(String[] args) {
		//第一种算法在n比较大的时候会出现错误
		System.out.println(fib1(30)); 
	}
}
